{
  "compilerOptions": {
    "target": "ES2019",
    "module": "CommonJS",
    "jsx": "react",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true
  },
  "include": ["hooks/**/*", "src/**/*"],
  "exclude": ["node_modules"]
}
